local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()
local win = DiscordLib:Window("PotatoHub Universal")

local plr = game.Players.LocalPlayer
local chr = plr.Character
local hum = chr:WaitForChild("Humanoid")

function animate(anim)

    local animp = hum:LoadAnimation(anim)

    animp:Play()

end

local rs = game.ReplicatedStorage

local anims = rs.Anims

local hub = win:Server("Hub", "")

local fcts = hub:Channel("Functions")

fcts:Button("Reset Anims", function()

    for i, v in hum:GetPlayingAnimationTracks() do

        v:Stop()

    end

end)

local hrs = win:Server("Heroes BattleGrounds", "")

local hrsanims = hrs:Channel("Animations")

hrsanims:Button("AsteroidHold", function()
DiscordLib:Notification("Animation Playing", "Animating...", "Ok!")
animate(anims.AsteroidHold)
end)

hrsanims:Button("BackDashEscape1", function()
DiscordLib:Notification("Animation Playing", "Animating...", "Ok!")
